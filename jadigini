     $(".variant-metafield").each(function(){
       const t = $(this)
       const id = t.attr("id")
       const target = $(`[data-select=${id}]`)
       t.on('select2:selecting', function(e) {
         const data = e.params.args.data
         target.val(data.text)
       });
     })
   })
