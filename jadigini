     })
 
     $("#cities").change(function(e){
       e.preventDefault()
       $(this).parent(".row").find("p.error").remove()
       $("#full-date").val("Full Calendar")
       $("#deliveryDate, #deliveryDateFull label ").show()
       $("#deliveryDateFull").removeClass("filled")
