<script>
const S=['A','K','Q','J','SCAT','WILD'];
const M=['NORMAL','TURBO','FAST'];
const D=document.getElementById('output');
function r(a){return a[Math.floor(Math.random()*a.length)];}

// Fungsi Grid Generator yang Diperbaiki (Responsif)
function gridGen(){
    let h='<div style="display:flex;flex-direction:column;gap:3px;margin-top:15px;border:1px dashed #FF5722;padding:8px;border-radius:6px;background:#0A2A4A;">';
    for(let rIdx=0;rIdx<3;rIdx++){
        // Menggunakan justify-content:space-between; pada baris
        h+='<div style="display:flex;justify-content:space-between;">';
        for(let cIdx=0;cIdx<5;cIdx++){
            // Perubahan penting: Menghilangkan min-width:45px
            // dan menggunakan flex: 1; margin: 0 1px; agar simbol membagi ruang rata (presisi)
            h+=`<span style="background:#1A3A5A;color:#FF8C00;padding:5px 0;border-radius:4px;font-weight:bold;text-align:center;font-size:12px;box-shadow: 0 0 5px rgba(255, 140, 0, 0.4);text-shadow: 0 0 3px #FF8C00;flex: 1; margin: 0 1px;">${r(S)}</span>`;
        }
        h+='</div>';
    }
    h+='</div>';
    return h;
}

function polaGen(){
if (!D) return;
let p='<ul style="list-style:none;padding:0;font-size:14px;margin-top:15px;line-height:2.0;text-align:left;">';
for(let i=0;i<3;i++){
p+=`<li><span style="color:#FF8C00;">[${i+1}]</span> <strong>${Math.floor(Math.random()*41)+10}x</strong> Spin <span style="color:#00FFFF;">${r(M)}</span></li>`;
}
p+='</ul>';
D.innerHTML = `
<div style="text-align:left;padding:10px 0;border-top:1px dashed #0A2A4A;margin-bottom:10px;">
<p style="margin:5px 0;font-size:13px;color:#e0e0e0;"><strong>GAME:</strong> <span style="color:#FF5722;">MAHJONG WAYS 2</span></p>
<p style="margin:5px 0;font-size:13px;color:#e0e0e0;"><strong>RTP_PREDICT:</strong> <span style="color:#39ff14;font-weight:bold;text-shadow: 0 0 5px #39ff14;">${(Math.random()*(99-90)+90).toFixed(2)}%</span></p>
</div>
<p style="font-size:13px;color:#00FFFF;margin:10px 0 5px 0;border-bottom:1px dashed #0A2A4A;padding-bottom:5px;">>> POLA SLOT GACOR HARI INI<<</p>
${p}
<p style="font-size:13px;margin:15px 0 5px 0;color:#FF5722;">>> **GRID_ANOMALY_SCAN**:</p>
${gridGen()}
<p style="font-size:11px;color:#777;margin-top:20px;">DATA GENERATED BY DEVILHAND</p>
`;
}
document.addEventListener('DOMContentLoaded', polaGen);
</script>

